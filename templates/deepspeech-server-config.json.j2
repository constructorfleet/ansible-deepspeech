{
  "deepspeech": {
    "model" :"{{ deepspeech_model_path }}",
    "alphabet": "{{ deepspeech_alphabet_path }}",
    "lm": "{{ deepspeech_language_model_path }}",
    "trie": "{{ deepspeech_trie_path }}",
    "features": {
      "n_features": 26,
      "n_context": 9,
      "beam_width": 500,
      "lm_alpha": 0.75,
      "lm_beta": 1.85
    }
  },
  "server": {
    "http": {
      "host": "0.0.0.0",
      "port": 8080,
      "request_max_size": 1048576
    }
  },
  "log": {
    "level": [
      { "logger": "deepspeech_server", "level": "DEBUG"}
    ]
  }
}